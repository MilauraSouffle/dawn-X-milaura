{% style %}
  /* ============================================
     MILAURA QUIZ - DIAGNOSTIC ÉMOTIONNEL
     ============================================ */

  main#MainContent > #shopify-section-{{ section.id }} {
    padding-top: 0 !important;
    padding-bottom: 0 !important;
  }

     #MilauraQuiz-{{ section.id }} {
    width: 100%;
    padding: 0 var(--milaura-spacing-sm) 0;
    background: transparent;
    min-height: 400px;
    margin-top: 0 !important;
    margin-bottom: 0 !important;
  }

  /* --- CONTENEUR PRINCIPAL --- */
  #MilauraQuiz-{{ section.id }} .quiz-container {
    /* Utilise la classe générique .milaura-section-card */
    /* max-width, width, margin gérés par .milaura-section-card */
    position: relative;
    /* Réduire le padding-top de la carte dorée pour coller au header */
    padding-top: var(--milaura-spacing-xs);
  }

  /* --- ÉTAT : INTRO --- */
  #MilauraQuiz-{{ section.id }} .quiz-state {
    display: none;
    opacity: 0;
    transform: translateX(40px);
    transition:
      opacity 520ms cubic-bezier(0.22, 1, 0.36, 1),
      transform 650ms cubic-bezier(0.22, 1, 0.36, 1);
  }

  /* Exit animation (slide left) */
  #MilauraQuiz-{{ section.id }} .quiz-state.is-exiting {
    transform: translateX(-40px);
    opacity: 0;
  }

  #MilauraQuiz-{{ section.id }} .quiz-state.is-active {
    display: block;
    opacity: 1;
    transform: translateX(0);
  }

  #MilauraQuiz-{{ section.id }} .quiz-intro-card {
    /* Le halo est géré par le conteneur parent .milaura-section-card */
    background: transparent;
    border: none;
    border-radius: 0;
    padding: 0;
    box-shadow: none;
    text-align: center;
  }

  #MilauraQuiz-{{ section.id }} .quiz-intro-title {
    font-family: var(--milaura-font-heading);
    color: var(--milaura-gold);
    font-size: 2.5rem;
    font-weight: 700;
    margin-bottom: var(--milaura-spacing-md);
    line-height: 1.2;
  }

  #MilauraQuiz-{{ section.id }} .quiz-intro-subtitle {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text);
    font-size: 1.1rem;
    line-height: 1.6;
    margin-bottom: var(--milaura-spacing-lg);
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-block {
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: blur(25px) saturate(180%);
    -webkit-backdrop-filter: blur(25px) saturate(180%);
    border: 1px solid rgba(192, 160, 98, 0.35);
    box-shadow: 0 10px 30px rgba(192, 160, 98, 0.15);
    border-radius: var(--milaura-border-radius-sm);
    padding: var(--milaura-spacing-md);
    margin: var(--milaura-spacing-lg) 0;
    display: flex;
    align-items: center;
    gap: var(--milaura-spacing-md);
    text-align: left;
    transition: all 0.3s ease;
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-block:hover {
    background: rgba(255, 255, 255, 0.8);
    box-shadow: 0 12px 35px rgba(192, 160, 98, 0.2);
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-image {
    width: 100px;
    height: 100px;
    border-radius: 50%;
    object-fit: cover;
    border: 2px solid var(--milaura-gold);
    flex-shrink: 0;
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-info {
    flex: 1;
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-name {
    font-family: var(--milaura-font-heading);
    color: var(--milaura-text);
    font-size: 1.3rem;
    font-weight: 700;
    margin-bottom: 5px;
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-title {
    font-family: var(--milaura-font-body);
    color: var(--milaura-gold);
    font-size: 1rem;
    text-transform: uppercase;
    letter-spacing: 1px;
    margin-bottom: 8px;
  }

  #MilauraQuiz-{{ section.id }} .quiz-expert-text {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text-light);
    font-size: 1.05rem;
    line-height: 1.5;
  }

  /* --- ÉTAT : QUESTIONS --- */
  #MilauraQuiz-{{ section.id }} .quiz-questions-header {
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: blur(20px);
    border-radius: var(--milaura-border-radius);
    padding: var(--milaura-spacing-md);
    margin-bottom: var(--milaura-spacing-lg);
    text-align: center;
    border: 1px solid rgba(192, 160, 98, 0.3);
  }

  #MilauraQuiz-{{ section.id }} .quiz-progress-text {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text);
    font-size: 1rem;
    margin-bottom: 10px;
    text-transform: uppercase;
    letter-spacing: 1px;
  }

  #MilauraQuiz-{{ section.id }} .quiz-progress-bar {
    width: 100%;
    height: 8px;
    background: rgba(192, 160, 98, 0.2);
    border-radius: 10px;
    overflow: hidden;
    position: relative;
  }

  #MilauraQuiz-{{ section.id }} .quiz-progress-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--milaura-gold) 0%, var(--milaura-gold-light) 100%);
    border-radius: 10px;
    transition: width 0.4s ease;
    box-shadow: 0 2px 10px rgba(192, 160, 98, 0.4);
    position: relative;
    overflow: hidden;
  }

  /* Shimmer effect on progress bar */
  #MilauraQuiz-{{ section.id }} .quiz-progress-fill::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    height: 100%;
    background: linear-gradient(
      120deg,
      transparent 0%,
      rgba(255, 255, 255, 0.6) 50%,
      transparent 100%
    );
    transform: translateX(-100%);
    animation: quiz-shimmer 2.5s ease-in-out infinite;
  }

  @keyframes quiz-shimmer {
    0% { transform: translateX(-100%); }
    100% { transform: translateX(100%); }
  }

  #MilauraQuiz-{{ section.id }} .quiz-question-card {
    /* Le halo est géré par le conteneur parent .milaura-section-card */
    background: transparent;
    border: none;
    border-radius: 0;
    padding: 0;
    box-shadow: none;
  }

  #MilauraQuiz-{{ section.id }} .quiz-question-title {
    font-family: var(--milaura-font-heading);
    color: var(--milaura-text);
    font-size: 2rem;
    font-weight: 700;
    margin-bottom: var(--milaura-spacing-md);
    text-align: center;
    line-height: 1.3;
  }

  #MilauraQuiz-{{ section.id }} .quiz-answers {
    display: grid;
    grid-template-columns: repeat(6, 1fr);
    gap: var(--milaura-spacing-md);
    margin-top: var(--milaura-spacing-lg);
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn {
    grid-column: span 2;
  }

  /* 3+2 centered layout: bottom row cards centered */
  #MilauraQuiz-{{ section.id }} .quiz-answer-btn:nth-child(4) {
    grid-column: 2 / span 2;
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn:nth-child(5) {
    grid-column: 4 / span 2;
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn {
    background: rgba(255, 255, 255, 0.85);
    backdrop-filter: blur(15px) saturate(160%);
    -webkit-backdrop-filter: blur(15px) saturate(160%);
    border: 2px solid rgba(192, 160, 98, 0.4);
    border-radius: var(--milaura-border-radius-sm);
    padding: var(--milaura-spacing-md);
    font-family: var(--milaura-font-body);
    color: var(--milaura-text);
    font-size: 1.15rem;
    font-weight: 600;
    cursor: pointer;
    text-align: center;
    min-height: 95px;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 8px 24px rgba(192, 160, 98, 0.1);
    /* Staggered entrance animation */
    opacity: 0;
    transform: translateY(20px);
    transition:
      opacity 380ms cubic-bezier(0.22, 1, 0.36, 1),
      transform 520ms cubic-bezier(0.22, 1, 0.36, 1),
      border-color 260ms ease,
      box-shadow 260ms ease,
      background 260ms ease;
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn.is-visible {
    opacity: 1;
    transform: translateY(0);
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn:hover {
    background: rgba(255, 255, 255, 0.95);
    border-color: rgba(192, 160, 98, 0.6);
    transform: translateY(-2px);
    box-shadow: 0 12px 32px rgba(192, 160, 98, 0.2);
  }

  #MilauraQuiz-{{ section.id }} .quiz-answer-btn:active {
    transform: translateY(0);
  }

  /* Pulse + gold border on click */
  #MilauraQuiz-{{ section.id }} .quiz-answer-btn.is-selected {
    animation: quiz-answer-pulse 600ms cubic-bezier(0.34, 1.56, 0.64, 1);
    border-color: var(--milaura-gold);
    background: rgba(255, 248, 225, 0.9);
    box-shadow:
      0 0 0 4px rgba(192, 160, 98, 0.3),
      var(--milaura-shadow-md);
  }

  @keyframes quiz-answer-pulse {
    0% {
      transform: scale(1);
      box-shadow: 0 0 0 0 rgba(192, 160, 98, 0);
    }
    40% {
      transform: scale(1.05);
      box-shadow: 0 0 0 8px rgba(192, 160, 98, 0.4);
    }
    100% {
      transform: scale(1);
      box-shadow: 0 0 0 4px rgba(192, 160, 98, 0.3);
    }
  }

  /* --- ÉTAT : TRANSITION --- */
  #MilauraQuiz-{{ section.id }} .quiz-transition-card {
    /* Le halo est géré par le conteneur parent .milaura-section-card */
    background: transparent;
    border: none;
    border-radius: 0;
    padding: 0;
    box-shadow: none;
    text-align: center;
    min-height: 300px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  #MilauraQuiz-{{ section.id }} .quiz-transition-title {
    font-family: var(--milaura-font-heading);
    color: var(--milaura-gold);
    font-size: 2.2rem;
    font-weight: 700;
    margin-bottom: var(--milaura-spacing-md);
    line-height: 1.3;
  }

  #MilauraQuiz-{{ section.id }} .quiz-transition-subtitle {
    font-family: var(--milaura-font-script);
    color: var(--milaura-text);
    font-size: 1.5rem;
    margin-bottom: var(--milaura-spacing-lg);
  }

  #MilauraQuiz-{{ section.id }} .quiz-transition-body {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text);
    font-size: 1.1rem;
    line-height: 1.7;
    max-width: 600px;
    margin: 0 auto;
  }

  #MilauraQuiz-{{ section.id }} .quiz-loader {
    display: flex;
    gap: 10px;
    margin-top: var(--milaura-spacing-lg);
    justify-content: center;
  }

  #MilauraQuiz-{{ section.id }} .quiz-loader-dot {
    width: 12px;
    height: 12px;
    background: var(--milaura-gold);
    border-radius: 50%;
    animation: quiz-loader-float 1.6s ease-in-out infinite;
  }

  #MilauraQuiz-{{ section.id }} .quiz-loader-dot:nth-child(2) {
    animation-delay: 0.3s;
  }

  #MilauraQuiz-{{ section.id }} .quiz-loader-dot:nth-child(3) {
    animation-delay: 0.6s;
  }

  @keyframes quiz-loader-float {
    0%, 100% {
      opacity: 0.4;
      transform: translateY(0) scale(1);
    }
    50% {
      opacity: 1;
      transform: translateY(-12px) scale(1.3);
    }
  }

  /* Fade text during loading */
  #MilauraQuiz-{{ section.id }} .quiz-transition-body,
  #MilauraQuiz-{{ section.id }} .quiz-transition-subtitle {
    animation: quiz-text-pulse 2.4s ease-in-out infinite;
  }

  @keyframes quiz-text-pulse {
    0%, 100% { opacity: 0.7; }
    50% { opacity: 1; }
  }

  /* ============================================
     ÉTAT : RESULT - DASHBOARD ÉMOTIONNEL V2
     Score + Cold Reading + Mantra + Offre + Duo + Rituel + Voyage
     ============================================ */

  /* Dashboard container */
  #MilauraQuiz-{{ section.id }} .quiz-dashboard {
    --profile-accent: var(--milaura-gold);
    --profile-accent-rgb: 192, 160, 98;
    max-width: 720px;
    margin: 0 auto;
    width: 100%;
    display: flex;
    flex-direction: column;
    gap: var(--milaura-spacing-lg);
  }

  /* === SECTION 1: PROFIL HEADER + SCORE === */
  #MilauraQuiz-{{ section.id }} .dash-header {
    display: flex;
    align-items: center;
    gap: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-score-wrap {
    position: relative;
    width: 100px;
    height: 100px;
    flex-shrink: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-score-svg {
    width: 100%;
    height: 100%;
    transform: rotate(-90deg);
  }

  #MilauraQuiz-{{ section.id }} .dash-score-bg {
    fill: none;
    stroke: rgba(var(--profile-accent-rgb), 0.12);
    stroke-width: 6;
  }

  #MilauraQuiz-{{ section.id }} .dash-score-fill {
    fill: none;
    stroke: var(--profile-accent);
    stroke-width: 6;
    stroke-linecap: round;
    stroke-dasharray: 264;
    stroke-dashoffset: 264;
    transition: stroke-dashoffset 2s cubic-bezier(0.4, 0, 0.2, 1);
  }

  #MilauraQuiz-{{ section.id }} .dash-score-value {
    position: absolute;
    inset: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
  }

  #MilauraQuiz-{{ section.id }} .dash-score-number {
    font-family: var(--milaura-font-heading);
    font-size: 2rem;
    font-weight: 700;
    color: var(--milaura-text);
    line-height: 1;
  }

  #MilauraQuiz-{{ section.id }} .dash-score-label {
    font-family: var(--milaura-font-body);
    font-size: 0.6rem;
    text-transform: uppercase;
    letter-spacing: 0.15em;
    color: var(--milaura-text-light);
    margin-top: 2px;
  }

  #MilauraQuiz-{{ section.id }} .dash-header-info {
    flex: 1;
  }

  #MilauraQuiz-{{ section.id }} .dash-profile-badge {
    display: inline-flex;
    align-items: center;
    gap: 6px;
    padding: 4px 12px;
    border-radius: var(--milaura-border-radius-lg);
    background: rgba(var(--profile-accent-rgb), 0.1);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.2);
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--profile-accent);
    margin-bottom: 8px;
  }

  #MilauraQuiz-{{ section.id }} .dash-profile-icon {
    font-size: 1rem;
  }

  #MilauraQuiz-{{ section.id }} .dash-hook {
    font-family: var(--milaura-font-heading);
    color: var(--milaura-text);
    font-size: 1.5rem;
    font-weight: 700;
    line-height: 1.25;
    margin: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-mood {
    font-family: var(--milaura-font-script);
    color: var(--profile-accent);
    font-size: 1.1rem;
    margin-top: 6px;
  }

  /* === SECTION 2: COLD READING === */
  #MilauraQuiz-{{ section.id }} .dash-reading {
    position: relative;
    padding: 0 0 0 var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-reading-line {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 3px;
    background: linear-gradient(to bottom, var(--profile-accent), transparent);
    border-radius: 4px;
  }

  #MilauraQuiz-{{ section.id }} .dash-reading-text {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text);
    font-size: 1.05rem;
    font-style: italic;
    line-height: 1.8;
    margin: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-reading-text .word {
    display: inline-block;
    opacity: 0;
    transform: translateY(6px);
    filter: blur(3px);
    transition: all 0.5s cubic-bezier(0.2, 0.8, 0.2, 1);
    margin-right: 0.25em;
  }

  #MilauraQuiz-{{ section.id }} .dash-reading-text .word.revealed {
    opacity: 1;
    transform: translateY(0);
    filter: blur(0);
  }

  /* === SECTION 3: MANTRA === */
  #MilauraQuiz-{{ section.id }} .dash-mantra {
    text-align: center;
    padding: var(--milaura-spacing-md);
    background: rgba(var(--profile-accent-rgb), 0.06);
    border-radius: var(--milaura-border-radius);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.12);
  }

  #MilauraQuiz-{{ section.id }} .dash-mantra-label {
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    text-transform: uppercase;
    letter-spacing: 0.2em;
    color: var(--profile-accent);
    margin-bottom: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-mantra-text {
    font-family: var(--milaura-font-script);
    font-size: 1.8rem;
    color: var(--milaura-text);
    text-shadow: 0 0 20px rgba(var(--profile-accent-rgb), 0.2);
    margin: 0;
    line-height: 1.4;
  }

  /* === SECTION 4: APPELS DE L'ÂME (produit) === */
  #MilauraQuiz-{{ section.id }} .dash-offer {
    background: rgba(255, 255, 255, 0.85);
    backdrop-filter: blur(24px) saturate(180%);
    -webkit-backdrop-filter: blur(24px) saturate(180%);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.3);
    border-radius: var(--milaura-border-radius);
    overflow: hidden;
    position: relative;
    box-shadow: 0 10px 40px rgba(var(--profile-accent-rgb), 0.15);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-accent {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    width: 4px;
    background: var(--profile-accent);
    z-index: 2;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-layout {
    display: flex;
    flex-direction: column;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-content {
    padding: var(--milaura-spacing-md);
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-badges {
    display: flex;
    align-items: center;
    gap: 8px;
    margin-bottom: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-badge-prio {
    font-family: var(--milaura-font-body);
    font-size: 0.6rem;
    font-weight: 700;
    background: var(--profile-accent);
    color: #fff;
    padding: 3px 8px;
    border-radius: 4px;
    text-transform: uppercase;
    letter-spacing: 1.5px;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-badge-profile {
    font-family: var(--milaura-font-body);
    font-size: 0.65rem;
    font-weight: 700;
    color: var(--profile-accent);
    text-transform: uppercase;
    letter-spacing: 1.5px;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-title {
    font-family: var(--milaura-font-heading);
    font-size: 1.5rem;
    font-weight: 700;
    color: var(--milaura-text);
    margin: 0 0 6px 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-desc {
    font-family: var(--milaura-font-body);
    font-size: 0.85rem;
    color: var(--milaura-text-light);
    line-height: 1.6;
    margin: 0 0 var(--milaura-spacing-md) 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-actions {
    display: flex;
    align-items: center;
    gap: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-cta {
    flex: 1;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    padding: 14px 20px;
    border-radius: var(--milaura-border-radius-lg);
    background: var(--milaura-text);
    color: #fff !important;
    font-family: var(--milaura-font-body);
    font-size: 0.75rem;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    text-decoration: none !important;
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
    box-shadow: 0 6px 20px rgba(0, 0, 0, 0.15);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-cta:hover {
    background: var(--profile-accent);
    transform: translateY(-2px);
    box-shadow: 0 8px 25px rgba(var(--profile-accent-rgb), 0.4);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-cta.is-added {
    background: #4CAF50;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-price {
    font-family: var(--milaura-font-heading);
    font-size: 1.4rem;
    font-weight: 700;
    color: var(--profile-accent);
    flex-shrink: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-image {
    position: relative;
    height: 180px;
    background: rgba(var(--profile-accent-rgb), 0.05);
    overflow: hidden;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-image img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: transform 0.7s ease;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer:hover .dash-offer-image img {
    transform: scale(1.05);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-image-fade {
    position: absolute;
    inset: 0;
    background: linear-gradient(to bottom, transparent 50%, rgba(255, 255, 255, 0.9));
    pointer-events: none;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-link {
    display: block;
    text-align: center;
    padding: 10px;
    font-family: var(--milaura-font-body);
    font-size: 0.85rem;
    font-weight: 600;
    color: var(--profile-accent) !important;
    text-decoration: underline !important;
    transition: color 0.3s ease;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-link:hover {
    color: var(--milaura-text) !important;
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-trust {
    display: flex;
    justify-content: center;
    gap: var(--milaura-spacing-md);
    padding: var(--milaura-spacing-sm);
    border-top: 1px solid rgba(var(--profile-accent-rgb), 0.08);
  }

  #MilauraQuiz-{{ section.id }} .dash-offer-trust-item {
    display: flex;
    align-items: center;
    gap: 4px;
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    color: var(--milaura-text-light);
  }

  /* === SECTION 5: DUO PIERRE + SENTEUR === */
  #MilauraQuiz-{{ section.id }} .dash-duo {
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.15);
    border-radius: var(--milaura-border-radius);
    padding: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-title {
    font-family: var(--milaura-font-heading);
    font-size: 0.75rem;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--profile-accent);
    text-align: center;
    margin-bottom: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-grid {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: var(--milaura-spacing-sm);
    margin-bottom: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-card {
    background: rgba(255, 255, 255, 0.6);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.12);
    border-radius: var(--milaura-border-radius-sm);
    padding: var(--milaura-spacing-md) var(--milaura-spacing-sm);
    text-align: center;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-card:hover {
    transform: translateY(-2px);
    box-shadow: 0 6px 20px rgba(var(--profile-accent-rgb), 0.12);
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-icon {
    font-size: 1.8rem;
    margin-bottom: 6px;
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-label {
    font-family: var(--milaura-font-body);
    font-size: 0.65rem;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    color: var(--profile-accent);
    font-weight: 600;
    margin-bottom: 2px;
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-name {
    font-family: var(--milaura-font-heading);
    font-size: 1rem;
    font-weight: 700;
    color: var(--milaura-text);
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-benefits {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-benefits li {
    padding: 4px 0 4px 20px;
    position: relative;
    font-family: var(--milaura-font-body);
    font-size: 0.85rem;
    color: var(--milaura-text);
    line-height: 1.5;
  }

  #MilauraQuiz-{{ section.id }} .dash-duo-benefits li::before {
    content: '\2727';
    position: absolute;
    left: 0;
    color: var(--profile-accent);
  }

  /* === SECTION 6: RITUEL === */
  #MilauraQuiz-{{ section.id }} .dash-ritual {
    background: rgba(255, 255, 255, 0.7);
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.15);
    border-radius: var(--milaura-border-radius);
    padding: var(--milaura-spacing-md);
