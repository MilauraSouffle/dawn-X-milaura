    function handleAnswerClick(buttonEl, profile) {
      // Disable all buttons during animation
      const allBtns = container.querySelectorAll('.quiz-answer-btn');
      allBtns.forEach(b => b.style.pointerEvents = 'none');

      // Apply pulse animation
      buttonEl.classList.add('is-selected');

      // Capture question 7 answer BEFORE incrementing (for tiebreaker rule)
      if (currentQuestionIndex === 6) { // Question 7 (0-indexed)
        question7Answer = profile;
      }

      // Wait for pulse animation (600ms), then advance
      setTimeout(() => {
        if (profiles.includes(profile)) {
          scores[profile] = (scores[profile] || 0) + 1;
        }
        currentQuestionIndex++;
        displayQuestion(currentQuestionIndex);
      }, 600);
    }

    // Legacy function for compatibility
    function handleAnswer(profile) {
      if (profiles.includes(profile)) {
        scores[profile] = (scores[profile] || 0) + 1;
      }

      currentQuestionIndex++;
      displayQuestion(currentQuestionIndex);
    }

    // ============================================
    // CALCUL DES RÉSULTATS
    // ============================================
    function calculateResults() {
      const totalScore = Object.values(scores).reduce((sum, val) => sum + val, 0);
      const percentages = {};
      let maxScore = -1;
      let tiedProfiles = [];

      // First pass: find max score and collect tied profiles
      profiles.forEach(profile => {
        const score = scores[profile] || 0;
        percentages[profile] = totalScore > 0 ? Math.round((score / totalScore) * 100) : 0;
        if (score > maxScore) {
          maxScore = score;
          tiedProfiles = [profile];
        } else if (score === maxScore) {
          tiedProfiles.push(profile);
        }
      });

      // Apply tiebreaker rule: if there's a tie, use question 7 answer
      let winnerProfile = tiedProfiles[0];
      if (tiedProfiles.length > 1 && question7Answer && tiedProfiles.includes(question7Answer)) {
        winnerProfile = question7Answer;
      }

      return {
        winnerProfile: winnerProfile,
        scores: scores,
        percentages: percentages,
        totalScore: totalScore
      };
    }

    // ============================================
    // AFFICHAGE DES RÉSULTATS - HERO HORIZONTAL
    // Affichage direct, pas de phases
    // ============================================

    // Stone handle mapping for cross-sell sections
    const stoneHandleMap = {
      apaisement: 'calcedoine-bleue',
      protection: 'obsidienne-noire',
      serenite: 'amethyste',
      amour: 'quartz-rose',
      chance: 'aventurine-verte'
    };

    function displayResults() {
      const results = calculateResults();
      const winnerProfile = results.winnerProfile;

      const profile = profileData[winnerProfile];
      if (!profile) return;

      // Set CSS custom properties on dashboard
      const dashboard = container.querySelector('.quiz-dashboard');
      if (dashboard) {
        dashboard.style.setProperty('--profile-accent', profile.colorAccent || '#C0A062');
        dashboard.style.setProperty('--profile-accent-rgb', profile.colorAccentRgb || '192, 160, 98');
      }

      // Also set on sticky (outside dashboard flow)
      const stickyEl = container.querySelector('.dash-sticky');
      if (stickyEl) {
        stickyEl.style.setProperty('--profile-accent', profile.colorAccent || '#C0A062');
        stickyEl.style.setProperty('--profile-accent-rgb', profile.colorAccentRgb || '192, 160, 98');
      }

      // === 1. HEADER ===
      const iconEl = container.querySelector('#dash-icon-' + sectionId);
      if (iconEl) iconEl.textContent = profile.icon || '\u2727';

      const profileNameEl = container.querySelector('#dash-profile-name-' + sectionId);
      if (profileNameEl) profileNameEl.textContent = (profile.label || '').toUpperCase();

      const hookEl = container.querySelector('#dash-hook-' + sectionId);
      if (hookEl) hookEl.textContent = profile.dashboardTitle || '';

      const moodEl = container.querySelector('#dash-mood-' + sectionId);
      if (moodEl) moodEl.textContent = profile.moodLabel || '';

      // === 2. COLD READING (word-by-word animation) ===
      const readingEl = container.querySelector('#dash-reading-' + sectionId);
      if (readingEl && profile.description) {
        var words = profile.description.split(/\s+/);
        readingEl.innerHTML = words.map(function(w) { return '<span class="word">' + w + '</span>'; }).join('');
        // Animate after state transition completes
        setTimeout(function() {
          var wordSpans = readingEl.querySelectorAll('.word');
          wordSpans.forEach(function(span, i) {
            setTimeout(function() { span.classList.add('revealed'); }, i * 60);
          });
        }, 800);
      }

      // === 3. MANTRA ===
      const mantraEl = container.querySelector('#dash-mantra-' + sectionId);
      if (mantraEl && profile.mantra) mantraEl.textContent = '\u00AB ' + profile.mantra + ' \u00BB';

      // === 4. SCORE ANIMATION ===
      const maxScore = Math.max.apply(null, Object.values(results.scores));
      const vibrationScore = Math.round((maxScore / questionsData.length) * 100);
      const scoreCircle = container.querySelector('#dash-score-circle-' + sectionId);
      const scoreNumber = container.querySelector('#dash-score-number-' + sectionId);

      if (scoreCircle) {
        var circumference = 2 * Math.PI * 42;
        scoreCircle.style.strokeDasharray = circumference;
        scoreCircle.style.strokeDashoffset = circumference;
        setTimeout(function() {
          scoreCircle.style.strokeDashoffset = circumference - (circumference * vibrationScore / 100);
        }, 400);
      }

      if (scoreNumber) {
        var startTime = performance.now();
        var duration = 2000;
        var animateNum = function(now) {
          var elapsed = now - startTime;
          var progress = Math.min(elapsed / duration, 1);
          var eased = 1 - Math.pow(1 - progress, 3);
          scoreNumber.textContent = Math.floor(eased * vibrationScore);
          if (progress < 1) requestAnimationFrame(animateNum);
        };
        setTimeout(function() { requestAnimationFrame(animateNum); }, 400);
      }

      // === 5. PRODUCT OFFER ===
      const offerProfileEl = container.querySelector('#dash-offer-profile-' + sectionId);
      if (offerProfileEl) offerProfileEl.textContent = 'P\u00F4le ' + (profile.displayName || profile.label);

      const offerTitleEl = container.querySelector('#dash-offer-title-' + sectionId);
      if (offerTitleEl) offerTitleEl.textContent = profile.moodLabel || '';

      const offerDescEl = container.querySelector('#dash-offer-desc-' + sectionId);
      if (offerDescEl) {
        var shortDesc = (profile.description || '').split('.').slice(0, 2).join('.') + '.';
        offerDescEl.textContent = shortDesc;
      }

      if (profile.product) {
        var imgEl = container.querySelector('#dash-offer-img-src-' + sectionId);
        if (imgEl && profile.product.featured_image) {
          imgEl.src = profile.product.featured_image;
          imgEl.alt = profile.product.title || '';
        } else {
          var imgWrap = container.querySelector('#dash-offer-img-' + sectionId);
          if (imgWrap) imgWrap.style.display = 'none';
        }

        var priceEl = container.querySelector('#dash-offer-price-' + sectionId);
        if (priceEl && profile.product.price) priceEl.textContent = profile.product.price;

        var ctaBtn = container.querySelector('#dash-offer-cta-' + sectionId);
        if (ctaBtn && profile.product.variant_id) {
          var ctaLabel = '{{ section.settings.result_cta_label | default: "Adopter mon rituel" }}';
          ctaBtn.textContent = ctaLabel + ' \u2192';
          ctaBtn.addEventListener('click', function() {
            addToCartWithFeedback(profile.product.variant_id, ctaBtn);
          });
        }

        var linkEl = container.querySelector('#dash-offer-link-' + sectionId);
        if (linkEl && profile.product.handle) {
          linkEl.href = '/products/' + profile.product.handle + '?view=milaura-produit';
        }

        // Sticky CTA content
        var stickyProduct = container.querySelector('#dash-sticky-product-' + sectionId);
        if (stickyProduct) stickyProduct.textContent = profile.product.title || ('Bougie ' + profile.label);

        var stickyBtn = container.querySelector('#dash-sticky-btn-' + sectionId);
        if (stickyBtn) {
          stickyBtn.textContent = 'Ajouter \u2022 ' + (profile.product.price || '');
          if (profile.product.variant_id) {
            stickyBtn.addEventListener('click', function() {
              addToCartWithFeedback(profile.product.variant_id, stickyBtn);
            });
          }
        }
      }

      // === 6. DUO ===
      const stoneEl = container.querySelector('#dash-stone-' + sectionId);
      if (stoneEl) stoneEl.textContent = profile.stone || '-';

      const scentEl = container.querySelector('#dash-scent-' + sectionId);
      if (scentEl) scentEl.textContent = profile.scent || '-';

      const benefitsEl = container.querySelector('#dash-benefits-' + sectionId);
      if (benefitsEl && profile.bullets) {
        var tempDiv = document.createElement('div');
        tempDiv.innerHTML = profile.bullets;
        var liElements = tempDiv.querySelectorAll('li');
        var html = '';
        liElements.forEach(function(li) { html += '<li>' + li.textContent + '</li>'; });
        benefitsEl.innerHTML = html;
      }

      // === 7. RITUAL STEPS ===
      const ritualEl = container.querySelector('#dash-ritual-steps-' + sectionId);
      if (ritualEl && profile.ritual) {
        var steps = profile.ritual.split('\n').filter(function(s) { return s.trim(); });
        var ritualHtml = '';
        steps.forEach(function(step, i) {
          var text = step.replace(/^\d+\.\s*/, '');
          ritualHtml += '<div class="dash-ritual-step"><div class="dash-ritual-num">' + (i + 1) + '</div><div class="dash-ritual-text">' + text + '</div></div>';
        });
        ritualEl.innerHTML = ritualHtml;
      }

      // === 8. VOYAGE FLIP CARDS ===
      // Card 1: Ritual
      var vc1Title = container.querySelector('#dash-vc1-title-' + sectionId);
      if (vc1Title) vc1Title.textContent = 'Rituel ' + (profile.displayName || profile.label);

      var vc1Back = container.querySelector('#dash-vc1-back-' + sectionId);
      if (vc1Back && profile.ritual) {
        vc1Back.innerHTML = profile.ritual.split('\n').filter(function(s) { return s.trim(); }).map(function(s) { return '<p style="margin:0 0 8px">' + s + '</p>'; }).join('');
      }

      // Card 2: Bilan
      var vc2Sub = container.querySelector('#dash-vc2-sub-' + sectionId);
      if (vc2Sub) vc2Sub.textContent = vibrationScore + '/100';

      var vc2Desc = container.querySelector('#dash-vc2-desc-' + sectionId);
      if (vc2Desc) vc2Desc.textContent = 'Vibration dominante : ' + (profile.displayName || profile.label);

      var vc2Back = container.querySelector('#dash-vc2-back-' + sectionId);
      if (vc2Back) {
        var bilanHtml = '';
        Object.keys(results.percentages).forEach(function(key) {
          var pData = profileData[key];
          if (pData) {
            bilanHtml += '<p style="margin:0 0 4px;font-size:0.8rem">' + pData.icon + ' ' + pData.label + ' : ' + results.percentages[key] + '%</p>';
          }
        });
        vc2Back.innerHTML = bilanHtml;
      }

      // Card 3: Duo
      var vc3Icon = container.querySelector('#dash-vc3-icon-' + sectionId);
      if (vc3Icon) vc3Icon.textContent = profile.icon || '\uD83D\uDC8E';

      var vc3Title = container.querySelector('#dash-vc3-title-' + sectionId);
      if (vc3Title) vc3Title.textContent = (profile.stone || 'Pierre') + ' & ' + (profile.scent || 'Senteur');

      var vc3Sub = container.querySelector('#dash-vc3-sub-' + sectionId);
      if (vc3Sub) vc3Sub.textContent = 'Ton alchimie';

      var vc3Back = container.querySelector('#dash-vc3-back-' + sectionId);
      if (vc3Back && profile.bullets) {
        var tempDiv2 = document.createElement('div');
        tempDiv2.innerHTML = profile.bullets;
        var lis = tempDiv2.querySelectorAll('li');
        var duoHtml = '<p style="margin:0 0 8px;font-size:0.85rem"><strong>' + (profile.stone || '') + '</strong> + <strong>' + (profile.scent || '') + '</strong></p>';
        lis.forEach(function(li) { duoHtml += '<p style="margin:0 0 4px;font-size:0.8rem">\u2727 ' + li.textContent + '</p>'; });
        vc3Back.innerHTML = duoHtml;
      }

      // === ACCOUNT ===
      const accountEl = container.querySelector('#dash-account-' + sectionId);
      if (accountEl) accountEl.style.display = isCustomerLoggedIn ? 'none' : '';

      // === STICKY CTA SCROLL LOGIC ===
      var offerRef = container.querySelector('#dash-offer-' + sectionId);
      var stickyRef = container.querySelector('#dash-sticky-' + sectionId);
      if (offerRef && stickyRef) {
        var handleStickyScroll = function() {
          var rect = offerRef.getBoundingClientRect();
          if (rect.bottom < 100) {
            stickyRef.classList.add('is-visible');
          } else {
            stickyRef.classList.remove('is-visible');
          }
        };
        window.addEventListener('scroll', handleStickyScroll, { passive: true });
      }

      // === STOCKAGE LOCAL ===
      const storageData = {
        profileId: winnerProfile,
        profileName: profile.displayName || profile.label,
        stone: profile.stone,
        scent: profile.scent,
        stoneHandle: stoneHandleMap[winnerProfile] || null,
        mantra: profile.mantra || null,
        ritual: profile.ritual || null,
        profileAccent: profile.colorAccent || '#C0A062',
        profileAccentRgb: profile.colorAccentRgb || '192, 160, 98',
        scores: results.scores,
        percentages: results.percentages,
        productHandle: profile.product ? profile.product.handle : null,
        productTitle: profile.product ? profile.product.title : null,
        timestamp: Date.now()
      };
      localStorage.setItem('milauraLastResult', JSON.stringify(storageData));

      // Dispatch event so downstream sections can react
      window.dispatchEvent(new CustomEvent('milaura:quiz-result', { detail: storageData }));
    }

    // Fonction globale pour ajouter au panier avec feedback visuel
    window.addToCartWithFeedback = function(variantId, buttonEl) {
      const originalText = buttonEl.textContent;
      buttonEl.textContent = 'Ajout en cours...';
      buttonEl.disabled = true;

      fetch('/cart/add.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ id: variantId, quantity: 1 })
      })
      .then(response => response.json())
      .then(() => {
        buttonEl.textContent = '✓ Ajouté au panier';
        buttonEl.style.background = 'linear-gradient(135deg, #4CAF50, #66BB6A)';

        // Remettre le bouton normal après 2s
        setTimeout(() => {
          buttonEl.textContent = originalText;
          buttonEl.style.background = '';
          buttonEl.disabled = false;
        }, 2000);

        // Mettre à jour le compteur du panier si présent
        if (typeof window.updateCartCount === 'function') {
          window.updateCartCount();
        }
      })
      .catch(error => {
        console.error('Erreur ajout panier:', error);
        buttonEl.textContent = 'Erreur - Réessayer';
        buttonEl.style.background = 'linear-gradient(135deg, #f44336, #e57373)';
        buttonEl.disabled = false;

        setTimeout(() => {
          buttonEl.textContent = originalText;
          buttonEl.style.background = '';
        }, 2000);
      });
    };

    // ============================================
    // GESTION DU LOCALSTORAGE
    // ============================================
    function checkLastResult() {
      const lastResult = localStorage.getItem('milauraLastResult');
      if (lastResult) {
        try {
          const data = JSON.parse(lastResult);
          const linkEl = container.querySelector('#quiz-last-result-link-' + sectionId);
          const showLinkEl = container.querySelector('#quiz-show-last-' + sectionId);
          
          if (linkEl) linkEl.style.display = 'block';
          if (showLinkEl) {
            showLinkEl.addEventListener('click', (e) => {
              e.preventDefault();
              hasUserInteracted = true; // Activer le scroll
              // Recharger les résultats depuis localStorage
              const storedData = JSON.parse(localStorage.getItem('milauraLastResult'));
              scores = storedData.scores;
              displayResults();
              goToState(QuizState.RESULT);
            });
          }
        } catch (e) {
          console.error('Error parsing last result:', e);
        }
      }
    }

    // ============================================
    // INITIALISATION
    // ============================================
    function init() {
      // Bouton start
      const startBtn = container.querySelector('#quiz-start-btn-' + sectionId);
      if (startBtn) {
        startBtn.addEventListener('click', () => {
          hasUserInteracted = true; // Activer le scroll après la première interaction
          goToQuestions();
        });
      }

      // Bouton restart
      const restartBtn = container.querySelector('#quiz-restart-btn-' + sectionId);
      if (restartBtn) {
        restartBtn.addEventListener('click', () => {
          scores = {
            apaisement: 0,
            protection: 0,
            serenite: 0,
            amour: 0,
            chance: 0
          };
          currentQuestionIndex = 0;
          question7Answer = null;
          goToQuestions();
        });
      }

      // Vérifier dernière révélation
      checkLastResult();

      // Afficher l'état initial
      goToState(QuizState.INTRO);
    }

    // Démarrer quand le DOM est prêt
    if (document.readyState === 'loading') {
      document.addEventListener('DOMContentLoaded', init);
    } else {
      init();
    }
  })();
</script>

