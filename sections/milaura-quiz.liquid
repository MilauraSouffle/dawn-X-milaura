  #MilauraQuiz-{{ section.id }} .dash-ritual-title {
    font-family: var(--milaura-font-heading);
    font-size: 0.75rem;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 2px;
    color: var(--profile-accent);
    text-align: center;
    margin-bottom: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-ritual-steps {
    display: flex;
    flex-direction: column;
    gap: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-ritual-step {
    display: flex;
    align-items: flex-start;
    gap: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-ritual-num {
    width: 28px;
    height: 28px;
    border-radius: 50%;
    background: rgba(var(--profile-accent-rgb), 0.12);
    color: var(--profile-accent);
