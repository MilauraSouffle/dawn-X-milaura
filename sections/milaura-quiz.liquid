    font-size: 0.85rem;
    font-weight: 700;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }

  #MilauraQuiz-{{ section.id }} .dash-ritual-text {
    font-family: var(--milaura-font-body);
    font-size: 0.95rem;
    color: var(--milaura-text);
    line-height: 1.6;
    padding-top: 3px;
  }

  /* === SECTION 7: MON VOYAGE (Flip Cards) === */
  #MilauraQuiz-{{ section.id }} .dash-voyage-title {
    font-family: var(--milaura-font-heading);
    font-size: 1.3rem;
    color: var(--milaura-text);
    text-align: center;
    margin-bottom: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-voyage-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: var(--milaura-spacing-sm);
    perspective: 1200px;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-card {
    position: relative;
    height: 280px;
    cursor: pointer;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-inner {
    position: relative;
    width: 100%;
    height: 100%;
    transition: transform 0.7s cubic-bezier(0.4, 0, 0.2, 1);
    transform-style: preserve-3d;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-card.is-flipped .dash-flip-inner {
    transform: rotateY(180deg);
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-front,
  #MilauraQuiz-{{ section.id }} .dash-flip-back {
    position: absolute;
    width: 100%;
    height: 100%;
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
    border-radius: var(--milaura-border-radius);
    padding: var(--milaura-spacing-md) var(--milaura-spacing-sm);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
    box-sizing: border-box;
    background: rgba(255, 255, 255, 0.85);
    backdrop-filter: blur(24px) saturate(180%);
    -webkit-backdrop-filter: blur(24px) saturate(180%);
    border: 1px solid rgba(var(--profile-accent-rgb), 0.15);
    box-shadow: 0 8px 24px rgba(var(--profile-accent-rgb), 0.1);
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-back {
    transform: rotateY(180deg);
    background: rgba(253, 251, 247, 0.95);
    border-color: rgba(var(--profile-accent-rgb), 0.25);
    justify-content: flex-start;
    padding-top: var(--milaura-spacing-md);
    text-align: left;
    overflow-y: auto;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-icon {
    font-size: 2.2rem;
    margin-bottom: var(--milaura-spacing-sm);
    filter: drop-shadow(0 4px 8px rgba(var(--profile-accent-rgb), 0.25));
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-title {
    font-family: var(--milaura-font-heading);
    font-size: 1rem;
    font-weight: 700;
    color: var(--milaura-text);
    margin-bottom: 4px;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-sub {
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    color: var(--profile-accent);
    margin-bottom: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-divider {
    width: 30px;
    height: 1px;
    background: rgba(var(--profile-accent-rgb), 0.3);
    margin-bottom: var(--milaura-spacing-sm);
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-desc {
    font-family: var(--milaura-font-body);
    font-size: 0.85rem;
    color: var(--milaura-text-light);
    line-height: 1.5;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-hint {
    margin-top: auto;
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    color: var(--profile-accent);
    opacity: 0.7;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-back-title {
    font-family: var(--milaura-font-heading);
    font-size: 0.9rem;
    font-weight: 700;
    color: var(--profile-accent);
    margin-bottom: var(--milaura-spacing-sm);
    padding-bottom: var(--milaura-spacing-xs);
    border-bottom: 1px solid rgba(var(--profile-accent-rgb), 0.15);
    width: 100%;
    text-align: center;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-back-text {
    font-family: var(--milaura-font-body);
    font-size: 0.85rem;
    color: var(--milaura-text);
    line-height: 1.7;
    font-style: italic;
  }

  #MilauraQuiz-{{ section.id }} .dash-flip-back-hint {
    margin-top: auto;
    font-family: var(--milaura-font-body);
    font-size: 0.7rem;
    color: var(--profile-accent);
    opacity: 0.7;
    padding-top: var(--milaura-spacing-sm);
    text-align: center;
    width: 100%;
  }

  /* === SECTION 8: FOOTER === */
  #MilauraQuiz-{{ section.id }} .dash-footer {
    text-align: center;
    padding-top: var(--milaura-spacing-md);
    border-top: 1px solid rgba(var(--profile-accent-rgb), 0.08);
  }

  #MilauraQuiz-{{ section.id }} .dash-restart {
    display: inline-flex;
    align-items: center;
    gap: 8px;
    color: var(--profile-accent);
    font-family: var(--milaura-font-body);
    font-size: 0.9rem;
    font-weight: 600;
    text-decoration: none;
    padding: 12px 24px;
    border: 1px solid rgba(var(--profile-accent-rgb), 0.3);
    border-radius: var(--milaura-border-radius-sm);
    background: transparent;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  #MilauraQuiz-{{ section.id }} .dash-restart:hover {
    background: rgba(var(--profile-accent-rgb), 0.08);
    border-color: var(--profile-accent);
  }

  #MilauraQuiz-{{ section.id }} .dash-account {
    margin-top: var(--milaura-spacing-md);
  }

  #MilauraQuiz-{{ section.id }} .dash-account-text {
    font-family: var(--milaura-font-body);
    color: var(--milaura-text-light);
    font-size: 0.85rem;
    margin-bottom: 6px;
  }

  #MilauraQuiz-{{ section.id }} .dash-account-link {
    display: inline-block;
    color: var(--profile-accent);
    font-weight: 600;
    font-size: 0.85rem;
    text-decoration: underline;
    transition: color 0.3s ease;
  }

  /* === STICKY CTA === */
  #MilauraQuiz-{{ section.id }} .dash-sticky {
    position: fixed;
    bottom: 20px;
    left: 50%;
    transform: translateX(-50%) translateY(100px);
    width: 90%;
    max-width: 400px;
    background: rgba(26, 26, 26, 0.95);
    backdrop-filter: blur(20px);
    -webkit-backdrop-filter: blur(20px);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: var(--milaura-border-radius-lg);
    padding: 8px 8px 8px 20px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: var(--milaura-spacing-sm);
    box-shadow: 0 20px 60px rgba(0, 0, 0, 0.3);
    z-index: 100;
    opacity: 0;
    transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1);
    pointer-events: none;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky.is-visible {
    transform: translateX(-50%) translateY(0);
    opacity: 1;
    pointer-events: auto;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-info {
    display: flex;
    flex-direction: column;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-label {
    font-family: var(--milaura-font-body);
    font-size: 0.55rem;
    text-transform: uppercase;
    letter-spacing: 0.15em;
    color: rgba(255, 255, 255, 0.5);
    font-weight: 700;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-product {
    font-family: var(--milaura-font-heading);
    font-size: 0.9rem;
    color: var(--profile-accent);
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-btn {
    padding: 10px 20px;
    border-radius: var(--milaura-border-radius-lg);
    background: var(--milaura-beige);
    color: var(--milaura-text) !important;
    font-family: var(--milaura-font-body);
    font-size: 0.65rem;
    font-weight: 700;
    text-transform: uppercase;
    letter-spacing: 1.5px;
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
    white-space: nowrap;
    text-decoration: none !important;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-btn:hover {
    background: var(--profile-accent);
    color: #fff !important;
  }

  #MilauraQuiz-{{ section.id }} .dash-sticky-btn.is-added {
    background: #4CAF50;
    color: #fff !important;
  }

  /* === DESKTOP === */
  @media screen and (min-width: 990px) {
    #MilauraQuiz-{{ section.id }} .quiz-dashboard {
      max-width: 800px;
    }

    #MilauraQuiz-{{ section.id }} .dash-hook {
      font-size: 1.8rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-mantra-text {
      font-size: 2.2rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-layout {
      flex-direction: row-reverse;
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-image {
      width: 40%;
      height: auto;
      min-height: 220px;
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-image-fade {
      background: linear-gradient(to right, rgba(255, 255, 255, 0.9) 0%, transparent 50%);
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-content {
      width: 60%;
    }
  }

  /* === MOBILE === */
  @media screen and (max-width: 989px) {
    #MilauraQuiz-{{ section.id }} .quiz-dashboard {
      gap: var(--milaura-spacing-md);
    }

    #MilauraQuiz-{{ section.id }} .dash-header {
      flex-direction: column-reverse;
      text-align: center;
    }

    #MilauraQuiz-{{ section.id }} .dash-hook {
      font-size: 1.4rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-reading-text {
      font-size: 0.95rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-mantra-text {
      font-size: 1.5rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-voyage-grid {
      grid-template-columns: 1fr;
      gap: var(--milaura-spacing-sm);
    }

    #MilauraQuiz-{{ section.id }} .dash-flip-card {
      height: 220px;
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-trust {
      gap: var(--milaura-spacing-sm);
    }
  }

  @media screen and (max-width: 480px) {
    #MilauraQuiz-{{ section.id }} .dash-score-wrap {
      width: 80px;
      height: 80px;
    }

    #MilauraQuiz-{{ section.id }} .dash-score-number {
      font-size: 1.5rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-hook {
      font-size: 1.25rem;
    }

    #MilauraQuiz-{{ section.id }} .dash-duo-grid {
      grid-template-columns: 1fr;
    }

    #MilauraQuiz-{{ section.id }} .dash-flip-card {
      height: 200px;
    }

    #MilauraQuiz-{{ section.id }} .dash-offer-trust {
      flex-direction: column;
      align-items: center;
      gap: 4px;
    }

    #MilauraQuiz-{{ section.id }} .dash-sticky {
      width: 94%;
      bottom: 12px;
      padding: 6px 6px 6px 16px;
    }
  }



  /* ============================================
     PERFORMANCE OPTIMIZATIONS
     ============================================ */

  /* GPU acceleration hints for animated elements */
  #MilauraQuiz-{{ section.id }} .quiz-state,
  #MilauraQuiz-{{ section.id }} .quiz-answer-btn,
  #MilauraQuiz-{{ section.id }} .quiz-progress-fill {
    will-change: transform, opacity;
  }

  /* Cleanup will-change after animation completes */
  #MilauraQuiz-{{ section.id }} .quiz-state.is-active,
  #MilauraQuiz-{{ section.id }} .quiz-answer-btn.is-visible {
    will-change: auto;
  }

  /* ============================================
     ACCESSIBILITY - REDUCED MOTION
     ============================================ */
  @media (prefers-reduced-motion: reduce) {
    #MilauraQuiz-{{ section.id }} *,
    #MilauraQuiz-{{ section.id }} *::before,
    #MilauraQuiz-{{ section.id }} *::after {
      animation-duration: 0.01ms !important;
      animation-iteration-count: 1 !important;
      transition-duration: 0.01ms !important;
    }
  }

  /* Fallback for browsers without backdrop-filter support */
  @supports not (backdrop-filter: blur(1px)) {
    #MilauraQuiz-{{ section.id }} .quiz-expert-block,
    #MilauraQuiz-{{ section.id }} .quiz-answer-btn,
    #MilauraQuiz-{{ section.id }} .quiz-reveal-duo-block,
    #MilauraQuiz-{{ section.id }} .quiz-reveal-duo-card {
      backdrop-filter: none !important;
      -webkit-backdrop-filter: none !important;
      background: rgba(255, 255, 255, 0.95) !important;
    }
  }
{% endstyle %}

<div id="MilauraQuiz-{{ section.id }}">
  <div class="quiz-container milaura-section-card">
    <!-- Lien dernière révélation (si existe) -->
    <div class="quiz-last-result-link" id="quiz-last-result-link-{{ section.id }}" style="display: none;">
      <a href="#" id="quiz-show-last-{{ section.id }}">Revoir ma dernière révélation ✨</a>
    </div>

    <!-- ÉTAT 1 : INTRO -->
    <div class="quiz-state quiz-state-intro" data-state="intro">
      <div class="quiz-intro-card">
        {% if section.settings.intro_title != blank %}
          <h2 class="quiz-intro-title milaura-heading">{{ section.settings.intro_title }}</h2>
        {% endif %}

        {% if section.settings.intro_subtitle != blank %}
          <div class="quiz-intro-subtitle milaura-text">{{ section.settings.intro_subtitle }}</div>
        {% endif %}

        {% if section.settings.show_expert and section.settings.expert_image != blank %}
          <div class="quiz-expert-block">
            <img
              src="{{ section.settings.expert_image | image_url: width: 200 }}"
              alt="{{ section.settings.expert_name }}"
              class="quiz-expert-image"
              width="200"
              height="200"
            >
            <div class="quiz-expert-info">
              {% if section.settings.expert_name != blank %}
                <div class="quiz-expert-name">{{ section.settings.expert_name }}</div>
              {% endif %}
              {% if section.settings.expert_title != blank %}
                <div class="quiz-expert-title">{{ section.settings.expert_title }}</div>
              {% endif %}
              {% if section.settings.expert_text != blank %}
                <div class="quiz-expert-text milaura-text">{{ section.settings.expert_text }}</div>
              {% endif %}
            </div>
          </div>
        {% endif %}

        {% if section.settings.intro_button_label != blank %}
          <button
            type="button"
            class="milaura-cta"
            id="quiz-start-btn-{{ section.id }}"
          >
            {{ section.settings.intro_button_label }}
          </button>
        {% endif %}
      </div>
    </div>

    <!-- ÉTAT 2 : QUESTIONS -->
    <div class="quiz-state quiz-state-questions" data-state="questions" style="display: none;">
      <div class="quiz-questions-header">
        <div class="quiz-progress-text" id="quiz-progress-text-{{ section.id }}">Question 1 sur 7</div>
        <div class="quiz-progress-bar">
          <div class="quiz-progress-fill" id="quiz-progress-fill-{{ section.id }}" style="width: 14.28%;"></div>
        </div>
      </div>

      <div class="quiz-question-card">
        <h3 class="quiz-question-title" id="quiz-question-title-{{ section.id }}"></h3>
        <div class="quiz-answers" id="quiz-answers-{{ section.id }}"></div>
      </div>
    </div>

    <!-- ÉTAT 3 : TRANSITION -->
    <div class="quiz-state quiz-state-transition" data-state="transition" style="display: none;">
      <div class="quiz-transition-card">
        {% if section.settings.transition_title != blank %}
          <h2 class="quiz-transition-title milaura-heading">{{ section.settings.transition_title }}</h2>
        {% endif %}

        {% if section.settings.transition_subtitle != blank %}
          <div class="quiz-transition-subtitle milaura-heading-script">{{ section.settings.transition_subtitle }}</div>
        {% endif %}

        {% if section.settings.transition_body != blank %}
          <div class="quiz-transition-body milaura-text">{{ section.settings.transition_body }}</div>
        {% endif %}

        <div class="quiz-loader">
          <div class="quiz-loader-dot"></div>
          <div class="quiz-loader-dot"></div>
          <div class="quiz-loader-dot"></div>
        </div>
      </div>
    </div>

    <!-- ÉTAT 4 : RESULT - DASHBOARD ÉMOTIONNEL V2 -->
    <div class="quiz-state quiz-state-result" data-state="result" style="display: none;">
      <div class="quiz-dashboard" id="quiz-dashboard-{{ section.id }}">

        <!-- 1. HEADER PROFIL + SCORE -->
        <div class="dash-header">
          <div class="dash-score-wrap">
            <svg class="dash-score-svg" viewBox="0 0 100 100">
              <circle class="dash-score-bg" cx="50" cy="50" r="42"></circle>
              <circle class="dash-score-fill" id="dash-score-circle-{{ section.id }}" cx="50" cy="50" r="42"></circle>
            </svg>
            <div class="dash-score-value">
              <span class="dash-score-number" id="dash-score-number-{{ section.id }}">0</span>
              <span class="dash-score-label">Vibration</span>
            </div>
          </div>
          <div class="dash-header-info">
            <div class="dash-profile-badge">
              <span class="dash-profile-icon" id="dash-icon-{{ section.id }}"></span>
              <span id="dash-profile-name-{{ section.id }}"></span>
            </div>
            <h2 class="dash-hook" id="dash-hook-{{ section.id }}"></h2>
            <div class="dash-mood" id="dash-mood-{{ section.id }}"></div>
          </div>
        </div>

        <!-- 2. COLD READING -->
        <div class="dash-reading">
          <div class="dash-reading-line"></div>
          <p class="dash-reading-text" id="dash-reading-{{ section.id }}"></p>
        </div>

        <!-- 3. MANTRA -->
        <div class="dash-mantra">
